<div class="col-sm-12 col-md-12">
  <div class="thumbnail">
    <div class="thumbnail-inner event-info-inner">
      <div class="caption">
        <div class="event-description">
          <h3><%= link_to post.title, post_path(post) %></h3>
          <h3><small><%= post.created_at %></small></h3>
          <h3><%= post.body %></h3>
        </div>
      </div>
    </div>
  </div>
</div>
